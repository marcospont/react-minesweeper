{
	"name": "react-minesweeper",
	"version": "1.0.0",
	"description": "React Minesweeper",
	"main": "index.js",
	"author": "Marcos Pont",
	"license": "MIT",
	"scripts": {
		"clean": "rimraf ./public ./build",
		"prestart": "npm run clean",
		"start": "nodemon --exec babel-node src/server/index.js --ignore src/client",
		"test:watch": "jest --watchAll --config=config/jest.config.js",
		"test:coverage": "jest --coverage --config=config/jest.config.js",
		"prebuild": "npm run clean",
		"build-webpack": "webpack -p --env.production --config config/webpack.config.js",
		"build-node": "babel src/server -d build --copy-files",
		"build": "npm run build-webpack && npm run build-node",
		"start-prod": "set NODE_ENV=production && set NODE_PATH=./build && pm2 start build/index.js -i max"
	},
	"devDependencies": {
		"@babel/cli": "^7.0.0-rc.1",
		"@babel/core": "^7.0.0-rc.1",
		"@babel/node": "^7.0.0-rc.1",
		"@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
		"@babel/plugin-proposal-decorators": "^7.0.0-rc.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
		"@babel/polyfill": "^7.0.0-rc.1",
		"@babel/preset-env": "^7.0.0-rc.1",
		"@babel/preset-react": "^7.0.0-rc.1",
		"autoprefixer": "^9.1.0",
		"babel-core": "^7.0.0-0",
		"babel-eslint": "^8.2.6",
		"babel-jest": "^23.4.2",
		"babel-loader": "^8.0.0-beta",
		"babel-plugin-array-includes": "^2.0.3",
		"css-loader": "^1.0.0",
		"eslint": "5.3.0",
		"eslint-config-airbnb": "17.1.0",
		"eslint-config-prettier": "^3.0.1",
		"eslint-plugin-import": "2.14.0",
		"eslint-plugin-jest": "^21.21.0",
		"eslint-plugin-jsx-a11y": "6.1.1",
		"eslint-plugin-prettier": "^2.6.2",
		"eslint-plugin-react": "7.11.1",
		"file-loader": "^1.1.11",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"postcss-loader": "^3.0.0",
		"precss": "^3.1.2",
		"prettier": "^1.14.2",
		"style-loader": "^0.22.1",
		"url-loader": "^1.1.1",
		"write-file-webpack-plugin": "^4.3.2"
	},
	"dependencies": {
		"@decorators/di": "^1.0.2",
		"@decorators/express": "^2.3.0",
		"autobind-decorator": "^2.1.0",
		"body-parser": "^1.18.3",
		"classnames": "^2.2.6",
		"compression": "^1.7.3",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"enzyme-to-json": "^3.3.4",
		"express": "^4.16.3",
		"global": "^4.3.2",
		"jest": "^23.4.2",
		"mobx": "^4.*",
		"mobx-react": "^5.2.5",
		"mobx-react-devtools": "^6.0.3",
		"pm2": "^3.0.3",
		"prop-types": "^15.6.2",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-favicon": "^0.0.14",
		"rimraf": "^2.6.2",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-dev-middleware": "^3.1.3",
		"webpack-hot-middleware": "^2.22.3"
	}
}
